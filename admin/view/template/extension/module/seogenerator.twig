	$(function(){
		$(".meta_value").click(function(){
			var btn_group = $(this).closest(".btn-group");
			var meta_value = $(this).attr('mv');
			var meta_field = $("#" + btn_group.attr('for'));
			meta_field.val(meta_field.val() + meta_value).focus();
			if($(this).prop("tagName") == 'A') {
				btn_group.find(".dropdown-toggle").dropdown('toggle');
			}
			return false;
		});
		$(".page_value").click(function(){
			var btn_group = $(this).closest(".btn-group");
			var page_value = $(this).attr('pv');
			$("." + btn_group.attr('for')).each(function(){
				$(this).val($(this).val() + page_value).focus();
			});
			return false;
		});
